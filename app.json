{
  "expo": {
    "name": "小狐狸打卡",
    "slug": "anqi-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash_real_fox_1260x2720.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff8e7"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2" 
    },
    "android": {
      "package": "com.anqiwang.foxtrainer",
      "versionCode": 2 ,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0c24b5d3-5607-4723-b074-433b15836b0e"
      }
    }
  }
}
